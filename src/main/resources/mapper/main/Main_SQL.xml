<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="main">
     <select id="selectBoardList" parameterMap="hashmap" resultMap="hashmap">
     	SELECT IDX
		     , TITLE
		     , HIT_CNT
		     , CREA_DTM
		  FROM TB_BOARD
		 WHERE DEL_GB = 'N'
		 ORDER BY IDX DESC
     </select>
</mapper>